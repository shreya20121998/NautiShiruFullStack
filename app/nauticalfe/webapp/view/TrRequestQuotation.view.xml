<mvc:View xmlns:unified="sap.ui.unified" xmlns:core="sap.ui.core" xmlns:tnt="sap.tnt"

xmlns:form="sap.ui.layout.form"

xmlns:l="sap.ui.layout"

controllerName="nauticalfe.controller.TrRequestQuotation"

    xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">

    <Page >
        <headerContent>
           <Bar class="customHeader">
          <contentLeft>
              <core:Icon
                  src="sap-icon://nav-back"
                  class="size1"
                  color="#FFFFFF"
                  press="onBackPress"
              />
              <Image
                  src="./images/ignxlogo.png"
                  height="75%"
              />
              <Text
                  class="textBid"
                  text="Request For Quotation"
              />
          </contentLeft>
          <contentRight>
              <core:Icon
                  src="sap-icon://search"
                  class="size1"
                  color="#FFFFFF"
              >
                  <core:layoutData>
                      <FlexItemData growFactor="1" />
                  </core:layoutData>
              </core:Icon>
              <core:Icon
                  src="sap-icon://SAP-icons-TNT/user"
                  class="size1"
                  color="#FFFFFF"
              >
                  <core:layoutData>
                      <FlexItemData growFactor="1" />
                  </core:layoutData>
              </core:Icon>
          </contentRight>
           </Bar>
        </headerContent>
        <Bar id="navbar">
            <contentLeft>
            <Button id="Button2" text="Sumbit Quotation"  />
            </contentLeft>
        </Bar>
         <VBox id="_IDGenVBox1" class="reqQuotation">
               <Text id="_IDGenText1" text="Bidding Details" class="vgheading "></Text>

                    <HBox id="_IDGenHBox1" alignItems="End">

                        <Label id="_IDGenLabel1" required="true" text="Chartering Request No :"   ></Label>

                        <Input id="_IDGenInput1" width="170px" showValueHelp="true" ></Input>

                    </HBox>

                    <HBox id="_IDGenHBox2" alignItems="End" >

                        <Label id="_IDGenLabel2" text="Bidding Start Date:"  required="true"></Label>

                        <DatePicker id="_IDGenDatePicker1" width="200px" placeholder="MM:DD:YY"></DatePicker>

                    </HBox>

                    <HBox id="_IDGenHBox3" alignItems="End" >

                         <Label id="_IDGenLabel3" text="Bidding Start Time:"  required="true" ></Label>

                         <TimePicker id="_IDGenTimePicker1" width="200px" placeholder="00:00:00"></TimePicker>

                    </HBox>

                    <HBox id="_IDGenHBox4" alignItems="End"  >

                         <Label id="_IDGenLabel4" text="Bidding End Date:"  required="true"></Label>

                         <DatePicker id="_IDGenDatePicker2" width="200px" placeholder="MM:DD:YY"></DatePicker>

                    </HBox>

                    <HBox id="_IDGenHBox5" alignItems="End" >

                        <Label id="_IDGenLabel5" text="Bidding End Time:"   required="true"></Label>

                        <TimePicker id="_IDGenTimePicker2" width="200px" placeholder="00:00:00"></TimePicker>

                    </HBox>

               </VBox>

    
    
    
    </Page>

</mvc:View>